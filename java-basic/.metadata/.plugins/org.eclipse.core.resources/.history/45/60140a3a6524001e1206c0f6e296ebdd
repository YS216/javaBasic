
 class Person5 {
	
	 
	 public int no;
	 public String name;
	 public String phone;
	 public String email;

	

	// 기능 : 메서드로 표현
	// public (접근 지정자 : 외부에서 무제한 접근 할 수 있음)
	public String info() {
		String info = String.format("[%d][%s][%s][%s]",  no, name, phone, email); // 내부 변수 접근
		return info;
	}
	
	////////////////////////////////////////////////////////////////////////////////////
	
}

public class PersonDemo5 {
	public static void main(String[] args) {
		
		
	}
}


